                {#<th>Notes</th>#}
                <th scope="col">Actions</th> 
           
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ entity.ruche }}</td>
                <td>{{ entity.date|date('d/m/Y') }}</td>
            {#    <td>{{ entity.activiteTrouEnvol }}</td>
