                <td>{{ entity.douceurEval }}</td>
                <td>{{ entity.hivernageEval }}</td>
                <td>{{ entity.propolisEval }}</td>
                <td>{{ entity.notes }}</td>#}
                <td> <a href="{{path('inspection_show', {'id': entity.id})}}" class="btn btn-default">Show</a></td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

     <ul>
          <div class="spacer-10"></div>
          <a href="{{ path('inspection_new') }}" class="btn btn-default"style="float:right">Ajouter</a>
    </ul>
