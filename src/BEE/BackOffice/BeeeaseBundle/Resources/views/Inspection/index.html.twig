{%extends "::layout.html.twig"%}

{% block headerTitle %}<h2>{{ headerTitle }}</h2>{% endblock %}

{% block window  -%}
 <div class="inner-padding">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th scope="col">Ruche</th>
                <th scope="col">Date</th>
                {#<th>Activité au trou d'envol</th>#}
                {#<th>Oeufs vus</th>#}
                {#<th>Reine vue</th>#}
                {#<th>Moisissure</th>#}
