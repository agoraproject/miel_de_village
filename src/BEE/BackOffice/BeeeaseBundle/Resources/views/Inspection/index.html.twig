                <td>{{ entity.reineVue }}</td>
                <td>{{ entity.moisissure }}</td>
                <td>{{ entity.abeillesSolVisite }}</td>
                <td>{{ entity.tMeteo }}</td>
                <td>{{ entity.vent }}</td>
                <td>{{ entity.tenueCadre }}</td>
                <td>{{ entity.cadreCouvainFerme }}</td>
                <td>{{ entity.cadreCouvainOuvert }}</td>
                <td>{{ entity.cadreCouvainMale }}</td>
                <td>{{ entity.cadreMiel }}</td>
                <td>{{ entity.cadrePollen }}</td>
                <td>{{ entity.peuplement }}</td>
                <td>{{ entity.repartitionCouvain }}</td>
                <td>{{ entity.qteCelluleRoyale }}</td>
                <td>{{ entity.celluleRoyaleEssaimage }}</td>
                <td>{{ entity.celluleRoyaleSupersedure }}</td>
                <td>{{ entity.celluleRoyaleSauvete }}</td>
                <td>{{ entity.poidsInspection }}</td>
                <td>{{ entity.abeillesTrainantes }}</td>
                <td>{{ entity.abeillesAilesDeformees }}</td>
