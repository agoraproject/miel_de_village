{%extends "::layout.html.twig"%}

{% block window  -%}
    <h1>Localisation</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Datedeplacement</th>
                <td>{{ entity.dateDeplacement|date('d/m/Y') }}</td>
            </tr>
            <tr>
                <th>Transhumance</th>
                <td>{{ entity.transhumance }}</td>
            </tr>
            <tr>
                <th>Noemplacement</th>
                <td>{{ entity.noEmplacement }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('localisation') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('localisation_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>

</ul>
{% endblock %}
