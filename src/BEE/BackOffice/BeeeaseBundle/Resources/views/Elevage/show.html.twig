{%extends "::layout.html.twig"%}

{% block window -%}
    <h1>Elevage</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Stadeintroduction</th>
                <td>{{ entity.stadeIntroduction }}</td>
            </tr>
            <tr>
                <th>Dateelevage</th>
                <td>{{ entity.dateElevage|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Nbregreffage</th>
                <td>{{ entity.nbreGreffage }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('elevage') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('elevage_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
